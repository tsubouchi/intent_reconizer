{
  "buildCommand": "cd apps/intent-router && pnpm install && pnpm build",
  "outputDirectory": "apps/intent-router/dist",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "apps/intent-router/dist/index.js": {
      "runtime": "@vercel/node@3.2.22"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/intent-router/dist/index.js"
    }
  ]
}
